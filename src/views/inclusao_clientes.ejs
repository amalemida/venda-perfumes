<html>
  <head>
    <%- include('../views/parciais/head'); %>
  </head>
  
  <body>
  
  <br><h1>Incluindo Novo CLIENTE</h1><br><br>
  
  <header class="cabecalhoFundo"></header>
  
  <div>
  <form action="/clientes/incluiClientes" method="post">
  
  <main class="conteudoPrincipal">
  <div class="tabelaAcesso">
  
  <input type="hidden" id="id" name="id" />
  
  <table class="table-mm">
  <tr>
    <td><label for="nome">Nome:</label></td>
    <td><input type="text" id="nome" name="nome" size="40" /></td>
  </tr>
  
  <tr>
    <td><label for="data">Aniversario:</label></td>
    <td><input type="text" id="niver" name="niver" size="10" /></td>
  </tr>
  
  <tr>
    <td><label for="email">Email:</label></td>
    <td><input type="text" id="email" name="email" size="40" /></td>
  </tr>
  
  <tr>
    <td><label for="cpf">CPF:</label></td>
    <td><input type="text" id="cpf" name="cpf" size="11" /></td>
  </tr>

  <tr>
    <td><label for="pedido">Pedido:</label></td>
    <td><input type="text" id="pedido" name="pedido" size="11" /></td>
  </tr>

  <tr>
    <td><label for="acesso">Acesso:</label></td>
    <td><input type="text" id="acesso" name="acesso" size="11" /></td>
  
  
  </table>
  
  <br>
     <input type="submit" value="Inserir Dados" class="btn btn-secondary" />
  </div>
  
  </main>
  </form>
  </div>    
  </body>
  <footer class="footer">
    <a class="btn btn-primary" role="button" href="../menu">Voltar ao menu</a>
  </footer>
  </html>
  